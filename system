# labyrinth 
i
 sh ~/labyrinth -i 

Escape
 sh ~/labyrinth -v 

alt + n
 sh ~/labyrinth -i 

t
 sh ~/labyrinth -n

a
 sh ~/labyrinth -a 
    
I
 sh ~/labyrinth -I 

d
 sh ~/labyrinth -d 

# dwm
w ; {1,2,3,4,5,6,7,8,9} 
 dwmc setlayoutex {1,2,3,4,5,6,7,8,9}

m ; {1-9} 
 dwmc tagex {0-9}

M
 dwmc togglemax

{1-9} 
 dwmc view {0-9}

{j,k}
 dwmc focusstack {+,-}1

{J,K}
 dwmc {pushdown,pushup}

# resize window left and right
{h,l}
 dwmc setmfact {-,+}0.05

Return
 dwmc zoom

W 
 dwmc togglebar

# kill windows. pleae. 
q
 dwmc killclient

s
 redshift -O 3000

S 
 redshift -x

# warpd
f
 warpd --hint

v
 warpd --normal


# Menus
Tab 
 rofi -show window
 
alt + F1
 rofi -show drun

ctrl + space
 rofi -show drun

#tools
e
 alacritty -e emacs -nw 

u
 alacritty 

F 
 alacritty -e ranger # default file manager.  

r
 alacritty -e sudo pacman -Syu

R
 python ~/cmdlaunch.py "sudo pacman -S"
    
P
 python ~/cmdlaunch.py sudo systemctl restart 

C  
 rog-control-center

b
 alacritty -e sudo bleachbit

p 
 alacritty -e sudo htop

shift + slash
 alacritty -e emacs -nw ~/.config/sxhkd/system