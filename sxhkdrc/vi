 alt + w ; {N,n}
  {dunstctl history-pop,dunstctl close-all}
# 
# alt + w ; c 
#  dwmc setlayoutex {0,1}
# 
 alt + w ; b 
  dwmc togglebar
# 
# alt + w ; {1-9}
#  dwmc view {0-9}
# 
 shift + space ; {1-9}
  dwmc tagex {0-9}
# 
 shift + space ; space 
  dwmc togglemax

alt + w ; Return
 dwmc zoom

alt + w ; o
 dwmc zoom

alt + w ; d 
 dwmc killclient

alt + w ; {n,N}  
 dunstctl {close-all,history-pop}

# [tools]

alt + q ; space 
 alacritty 

alt + q ; e
 alacritty

alt + q ; colon
 python ~/cmdlaunch.py ""

alt + q ; {w,W}
 {qutebrowser,torbrowser-launcher} 

alt + q ; d
 gnome-dictionary

alt + q ; ctrl + e
 goodvibes

alt + q ; c  
# book 
 manuskript 

alt + q ; n
 alacritty -e newsboat

alt + q ; m 
 alacritty -e ncmpcpp 

alt + q ; C 
 armcord 

# [productivity]  
# quick productivity functions.

alt + q ; d 
 python ~/cmdlaunch.py "python ~/t/t.py --task-dir ~/tasks --list tasks -d" 

