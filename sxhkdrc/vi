# [basic_system]

alt + w ; x 
 xkill 

alt + w ; P 
 python ~/cmdlaunch.py "pkill" 

shift + space ; space  
 slock 

# [config]

alt + q ; shift + slash
 alacritty -e vi ~/.config/sxhkd/vi
