# [basic_system]

alt + w ; x 
 xkill 

alt + w ; P 
 python ~/cmdlaunch.py "pkill" 

shift + space ; l  
 slock 

# [config]

alt + q ; slash
 st -e nvim ~/.config/sxhkd/vi
