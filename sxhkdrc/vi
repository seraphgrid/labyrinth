# [labyrinth]

alt + w ; ctrl + d  
# config
 labyrinth -c

alt + w ; a
# audio
 labyrinth -a

alt + w ; e
# normal
 labyrinth -v

# [nav]

alt + w ; slash
 rofi -show window -kb-element-prev 'k' -kb-element-next 'j' -kb-accept-entry 'o'

#alt + w ; I
 #rofi -show drun

alt + w ; I
 rofi -show drun

# [wm]

 alt + w ; {j,k}
  dwmc focusstack {+,-}1
