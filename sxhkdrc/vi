# [labyrinth]

alt + w ; ctrl + d  
# config
 labyrinth -c

alt + w ; a
# audio
 labyrinth -a

alt + w ; e
# normal
 labyrinth -v

# [nav]

alt + w ; slash
 rofi -show window -kb-element-prev 'k' -kb-element-next 'j' -kb-accept-entry 'o'

#alt + w ; I
 #rofi -show drun

alt + w ; I
 rofi -show drun

# [wm]

#alt + w ; {j,k}
# dwmc focusstack {+,-}1

#alt + q ; {j,k}
# dwmc focusstack {+,-}1

alt + {j,k}
 dwmc focusstack {+,-}1

alt + w ; {h,l}
 dwmc setmfact {-,+}0.05

shift + space ; {u,i}
 dwmc setcfact {-,+}0.85

shift + space ; o 
 dwmc setcfact

alt + w ; c 
 dwmc setlayoutex {0,1}

alt + w ; b 
 dwmc togglebar

alt + w ; {1-9}
 dwmc view {0-9}

shift + space ; {1-9}
 dwmc tagex {0-9}

alt + q ; {1-9}
 dwmc tagex {0-9}

alt + w ; {D,W}
 dwmc {pushdown,pushup} 

alt + {D,W}
 dwmc {pushdown,pushup} 

alt + w ; space 
 dwmc togglemax

alt + w ; Return
 dwmc zoom

alt + w ; o
 dwmc zoom

alt + w ; alt + w
 dwmc focusstack -1

alt + q ; alt + q
 dwmc focusstack +1

alt + w ; d 
 dwmc killclient

alt + q ; q 
 dwmc killclient

alt + w ; {n,N}  
alt + w ; s  
 redshift {-O 2444,-x}

# [tools]

alt + w ; q 
 st

alt + q ; e
 st -e nvim

alt + q ; colon
 python ~/cmdlaunch.py ""

alt + q ; {w,W}
 {qutebrowser,torbrowser-launcher} 

alt + q ; d
 gnome-dictionary

alt + q ; ctrl + e
 goodvibes

alt + q ; c  
# book 
 manuskript 

alt + q ; n
 st -e newsboat

alt + q ; m 
 st -e ncmpcpp 

alt + q ; space
 armcord 
# [productivity]  
# quick productivity functions.

alt + q ; d 
 python ~/cmdlaunch.py "python ~/t/t.py --task-dir ~/tasks --list tasks -d" 

