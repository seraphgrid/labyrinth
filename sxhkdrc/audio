alt + o ; shift + grave 
 dwmc setlayoutex {0,1}
 
alt + o ; F11 
 dwmc togglebar
 
alt + o ; {1-9} 
 dwmc viewex {0-8}

alt + o ; {g ; g,G}
 dwmc viewex {0,8}

alt + o ; space ; {1-9}
 dwmc tagex {0-8}

alt + o ; space ; {g ; g,G}
 dwmc tagex {0,8}

alt + o ; shift + grave
 dwmc togglemax

alt + o ; c ; w
 dwmc killclient; rofi -show drun

alt + o ; ctrl + n 
 dwmc nametag

alt + o ; space ; {j,k}
 dwmc rotatestack {+,-}1

alt + o ; ctrl + {j,k}
 dwmc setcfact {+,-}0.45

alt + o ; equal
 dwmc setcfact 

alt + o ; {l,h}
 dwmc setmfact {+,-}0.18

alt + f
 hints

alt + o ; f
 hints

alt + o ; r 
 hints -m scroll

alt + o ; V 
 warpd --hint

alt + o ; v 
 warpd --normal

alt + o ; z 
 dwmc zoom

alt + o ; y
 xdotool getactivewindow key "ctrl+c"

alt + o ; p
 xdotool getactivewindow key "ctrl+v"
 
alt + o ; q 
 dwmc killclient

alt + o ; {n,N}  
 dunstctl {close-all,history-pop}

alt + o ; e
 rofi -show window -kb-element-prev 'k' -kb-element-next 'j' -kb-accept-entry 'o'

alt + o ; colon 
 rofi -show drun

# [cmus]

alt + q ; M ; {r,R} 
