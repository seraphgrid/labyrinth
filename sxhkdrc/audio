 sh ~/labyrinth -v

I
 sh ~/labyrinth -I

i
 sh ~/labyrinth -i

t
 sh ~/labyrinth -t

n
 sh ~/labyrinth -n

d
 sh ~/labyrinth -d 

# dwm 
m ; {1-9} 
 dwmc tagex {0-9}

M
 dwmc togglemax

{1-9} 
 dwmc view {0-9}

{j,k}
 dwmc focusstack {+,-}1

{J,K}
 dwmc {pushdown,pushup}

{h,l}
 dwmc setmfact {-,+}0.05

{o,Return}
 dwmc zoom

N
 dwmc togglebar
q
 dwmc killclient

w
 dunstctl close-all

W
 dunstctl history-pop

# warpd
u
 warpd --hint

f
 warpd --hint

v
 warpd --normal

# audiosoftz.
